
/* 升级提示 */
.update_tip_mask{
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,.5);
	z-index: 99;
}
.update_tip{
	position: relative;
	width: 100%;
	height: 100%;
	top: 0;
}
.update_tip_title{
	font-family: 'PingFangSC-Regular';
	font-size: 5.466666666666667vw;
	color: #000000;
	letter-spacing: 0;
	text-align: center;
	padding-top: 13.066666666666666vw;
}
.update_tip_line{
	width: 36vw;
	height: 2rpx;
	background: #000000;
	margin: 0 auto;
	margin-top: 6rpx;
}
.update_tip_line:last-child{
	margin-top: 2rpx;
}
.update_tip_content{
	font-family: 'PingFangSC-Light';
	font-size: 4.533333333333333vw;
	color: #000000;
	letter-spacing: 0;
	text-align: center;
	line-height: 7.576vw;
	margin-top: 9.782666666666668vw;
}
.update_content_line{
	display: inline-block;
	margin: 0 auto;
	border-bottom: 1px dashed #818181;
}
.update_button{
	position: absolute;
	width: 23.066666666666666vw;
	height: 23.066666666666666vw;
	text-align: center;
	line-height: 23.066666666666666vw;
	border-radius: 50%;
	background: #FF7082;
	left: 0;
	right: 0;
	margin: auto;
	bottom: 8vw;
	overflow: hidden;
}
.update_button_after{
	content: '';
	position: absolute;
	width: 24.666666666666668vw;
	height: 24.666666666666668vw;
	text-align: center;
	line-height: 24.666666666666668vw;
	border-radius: 50%;
	border: 4px solid #FF7082;
	left: 0;
	right: 0;
	margin: auto;
	bottom: 6.2vw;
	box-shadow: 0 0 5px rgba(0,0,0,0.50);
}
.update_button>text{
	font-family: 'PingFangSC-Medium';
	font-size: 6.0600000000000005vw;
	color: #FFFFFF;
	letter-spacing: 0;
	text-align: center;
	line-height: 6.0600000000000005vw;
	text-shadow: 0 0 5px rgba(0,0,0,0.50);
	display: block;
	height: 6.0600000000000005vw;
	padding-top: 4.5vw;
}
.update_button>text:last-child{
	padding-top: 2vw;
}
.update_tip_panel{
	position: fixed;
	width:66.66666666666667vw;
	height: 91.2vw;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	background: url(data:image/svg+xml;base64,) center / cover no-repeat;
}