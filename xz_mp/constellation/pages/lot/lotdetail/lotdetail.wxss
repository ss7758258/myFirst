  color: rgb(158, 29, 29);
  font-size: 36rpx;
  box-shadow:0 0 3px rgba(0,0,0,.3);
}

.shareBox form {
  width: 80%;
  margin: 0 auto;
  color: rgb(158, 29, 29);
}

.invite .botton button {
  width: 100%;
}

.botton11 {
  width: 80%;
}

.chaiwanqianchuxian {
  animation: fennuzujianchuxian 2s linear;
}

.chaiwanqianxiaoshi {
  animation: xiaorongzujianxiaoshi 2s linear;
}
.UI_page{
  height:100%;
}

/* 幸运签淡出动画 */

@keyframes xiaorongzujianxiaoshi {
  0% {
    opacity: 1;
  }

  40% {
    opacity: 0.2;
  }

  100% {
    opacity: 0;
  }
}

@keyframes fennuzujianchuxian {
  0% {
    opacity: 0;
  }

  40% {
    opacity: 0.2;
  }

  100% {
    opacity: 1;
  }
}

/* 幸运签详情背景闪烁动画 */

@keyframes scale {
  0% {
    opacity: 1.2;
  }

  40% {
    opacity: 0.2;
  }

  100% {
    opacity: 1.2;
  }
}

/* 幸运签详情幸运签旋转动画 */

@keyframes rotate3d {
  0% {
    -webkit-transform: rotate3d(1, 1, 1, 0deg);
  }

  100% {
    -webkit-transform: rotate3d(1, 1, 1, 360deg);
  }
}

