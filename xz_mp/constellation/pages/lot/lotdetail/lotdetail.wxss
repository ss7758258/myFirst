}

.troops .nom {
}

.troops .nomPerch {
}

.troops .photo {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  background-color: rgba(255, 255, 255, 0.1);
}

.invite .number {
  font-size: 24rpx;
  text-align: center;
  margin: 1.3333333333333333vw 0 8vw 0;
}

.invite .botton {
  width: 80%;
  margin: 0 auto;
  color: rgb(158, 29, 29);
}

.invite .qipaobg {
  width: 80%;
  position: absolute;
  bottom: 13.4vh;
  left: 0;
  right: 0;
  margin:auto;
  z-index: 99;
  /* animation: qipao 3s linear; */
  /* transform: scale(1) translateY(0); */
  animation: qipao 2s ease-in-out forwards;
  transform-origin: bottom center;
}
/* .invite .qipaobg.show{
  transform: scale(0) translateY(-6.997001499250375vh);
} */
@keyframes qipao {
  0%,50%  {
    transform: scale(1) translateY(0);
  }
  100% {
    transform: scale(0) translateY(-6.997001499250375vh);
  }
}

.invite .qipaobg .qipaowenzi {
  position: absolute;
  left: 15%;
  top: 40%;
  font-size: 28rpx;
  width: 80%;
}

.shareBox {
