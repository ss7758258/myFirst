/* 自动占位置元素 */
.margin-panel{
    flex: 1;
}
.global_btn{
	display: block;
	color: #fff;
	margin: 0;
	padding: 0;
	height: 100%;
	background: rgba(0,0,0,0);
	text-align: inherit;
}
/**app.wxss**/
.panel_scene {
	padding: 2vw;
  	height: calc(100% - 4vw - 64px);
}

button::after {
	border: none;
}

.shouquan {
	position: fixed;
	top: 50%;
	left: 25%;
	z-index: 11;
}

page { 
	width: 100%;
	height: 100%;
	overflow: hidden; 
	font-size: 32rpx;
	color: white;
	/* background-image: linear-gradient(0deg, #8CBAFE 0%, #B672E2 52%, #9262FB 84%, #9262FB 99%); */
	/* background: url(data:image/svg+xml;base64,) no-repeat center ; */
	/* background-size: 50px; */
	background-color: #9262FB;
	font-family:PingFangSC;
}
.clearfix::after{
    content: "";
    clear: both;
    display: block;
    height: 0;
    overflow: hidden;
    visibility: hidden;
}

image {
	max-width: 100%;
}

.home {
	position: fixed;
	width: 100rpx;
	height: 100rpx;
	bottom: 20%;
	right: 4%;
}
.button1 {
	position: relative;
	display: inherit;
	margin-left: auto;
	margin-right: auto;
	padding-left: inherit;
	padding-right: inherit;
	box-sizing: inherit;
	font-size: inherit;
	text-align: inherit;
	text-decoration: inherit;
	line-height: inherit;
	border-radius: 0;
	-webkit-tap-highlight-color: inherit;
	overflow: inherit;
	color: inherit;
	background-color: inherit;
	border: none;
}

.button1::after {
	border: none;
}


.animateBtn{
	transform: scale(.9);
}