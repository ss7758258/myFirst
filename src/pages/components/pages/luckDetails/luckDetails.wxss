page{
    font-family: 'PingFangSC';
}
.luck-panel{
    width: 100%;
    max-height: calc(100% - 64px);
    display: flex;
    flex-direction: column;
    align-items: center;
}
.swiper_header{
    width: 750rpx;
    height: 108rpx; 
    position: fixed;
    left: 0;
    top: 128rpx; 
    z-index: 100;
    background: rgba(255, 255, 255, 1);
    font-weight:100;
}
.swiper_content{
    width: 750rpx;
    flex: 1;
    height: calc(100% - 100rpx);
    background: #fff;
}
.scroll-view{
    background: #fff;
    height: 100%;
    min-height: 100%;
    overflow: auto;
}

/* 导航栏 */
.header{
    height: 100rpx;
    border-bottom: 8rpx solid rgba(146,98,251,0.20); 
    background: rgba(255, 255, 255, 1);
    position: relative;
}
.head_son{
    width: 250rpx;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    font-size: 36rpx;
    color: #000;
    float: left; 
    border-bottom:8rpx solid #eee;
}
.head_active{
    width: 150rpx;
    height: 8rpx;
    border-radius: 100rpx;
    background: #9262FB;
    transition: all .4s 0s linear;
    position: absolute;
    bottom: -8rpx; 
}
.select{
    color: #9262FB;
}
.today{
     left: 50rpx; 
}
.thisweek{
     left: 300rpx; 
}
.thismonth{
     left: 550rpx; 
}


/* 运势指数 */
.index{
    margin: 30rpx 30rpx 0;
    padding-bottom: 40rpx;
    border-bottom: 1rpx dashed #9262FB;
}
/* 指数左侧 */
.index_left{
    float: left;
    max-width: 385rpx;
}
.synthesize,.luckycolor{
    font-size: 32rpx;
    color: #000000;
    margin-top: 20rpx;
    line-height: 45rpx;
}
.star_right{
    margin-right: 5rpx;
}
.view_star{
    display: inline-block;
    position: relative;
}
.star{
    width: 35rpx;
    height: 35rpx;
    margin-left: 5rpx;
    transform: translateY(5rpx);
}
.star_position{
    width: 35rpx;
    height: 35rpx;
    transform: translateY(5rpx);
}
.colon{
    display: inline-block;
    margin-right: 10rpx;
}
/* 指数右侧 */
.index_right{
    float: right;
    width: 280rpx;
}
.luckycolor{
    margin-left: 5rpx;
    margin-top: 17rpx;
}
.aries{
    display: block;
    width: 280rpx;  
    height: 120rpx;
    margin-top:15rpx;
    overflow: hidden;
}

/* 运势简评 */
.luckProfile{
    margin: 40rpx 30rpx 0;
    padding-bottom:30rpx; 
    border-bottom: 1rpx dashed #9262FB;
}
.luckProfile_head{
    height: 53rpx;
    margin-bottom: 30rpx;
}
.luckProfile_img{
    width: 50rpx;
    height: 50rpx;
    margin-right: 20rpx;
}
.luckProfile_text{
    font-size: 38rpx;
    color: #9262FB;
    font-weight: bold;
    position: relative;
    top: -10rpx; 
}
.luckProfile_content{
    font-size: 32rpx;
    color: #000000;
    line-height: 50rpx;
    font-weight: 400;
    transform: scale(.96);
}
.footer{
    height: 100rpx;
}
.love{
    color: #FD6A9A;
}
.work{
    color: #5B8DFD;
}
.wealth{
    color: #F76C1E;
}
.healthy{
    color: #85B3FF;
}
.daynotice{
    color: #6041CD;
}

.btn-show{
    position: fixed;
    bottom: 0;
    left: 0;
    text-align: center;
    width: 100%;
    height:100rpx;
    font-size:36rpx;
    font-family:PingFangSC;
    font-weight: 600;
    line-height:100rpx;
    color:rgba(255,255,255,1);
    background:linear-gradient(-90deg,rgba(174,69,255,1),rgba(146,98,251,1));
}
.ipx{
    padding-bottom: 34px;
}


/* 服务号弹窗 */

.contact_follow_mask {
	position: fixed;
	width: 100%;
	height: 100%;
	top: -100%;
	left: 0;
	background: rgba(0, 0, 0, .6);
	z-index: 99;
}

.contact_follow_mask.show {
	top: 0;
}

.contact_follow {
	position: absolute;
	top: -100%;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	width: 88vw;
	height: 93.33333333333333vw;
	/* transform: translateY(54px); */
	background: #fff;
	transition: all .15s ease-in-out;
	z-index: 100;
}
.collect_panel{
	background: initial;
	width: 670rpx;
	height: 690rpx;
	border-radius: 10rpx;
	transform: none;
}

.contact_follow.show {
	top: 0;
}

.contact_panel {
	position: relative;
	width: 100%;
	height: 100%;
}
.contact_panel>image{
	width: 100%;
	height: 100%;
	border-radius: 10rpx;
}
.contact_panel .collect-point{
	position: absolute;
	width: 150rpx;
	height: 150rpx;
	top: -160rpx;
	right: 0rpx;
}
button.collect-btn{
	padding-left:14px;
	padding-right:14px;
	margin : 0 auto;
	margin-top: 4.2vh;
	font-size: initial;
	width: 500rpx;
	height: 100rpx;
	line-height: 100rpx;
	background: #9262FB;
	border-radius: 10rpx;
	color: #fff;
	box-shadow: 0 0 5px rgba(0,0,0,.1);
	transition: all .1s linear;
}
.contact_head_panel {
	width: 100%;
	height: 37.333333333333336vw;
	overflow: hidden;
}

.contact_head_panel_2 {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 37.333333333333336vw;
}

.contact_head_panel_2 .contact_head {
	width: 100%;
	margin: auto;
	background: rgba(0, 0, 0, 0);
}

.contact_head {
	position: relative;
	width: 110%;
	margin-left: -5%;
	height: 37.333333333333336vw;
	border-radius: 0 0 50vw 50vw / 0 0 12vw 12vw;
	background: linear-gradient(0deg, #9262FB 0%, #bfa3fd 100%);
}

.contact_head_text {
	position: absolute;
	display: block;
	width: 65.33333333333333vw;
	height: 12.8vw;
	left: 0;
	right: 0;
	bottom: 0;
	top: 0;
	margin: auto;
	z-index: 1;
	font-family: PingFangSC-Regular;
	font-size: 4.533333333333333vw;
	color: #000000;
	letter-spacing: 0;
	text-align: center;
}

.contact_head_text .st {
	font-family: PingFangSC-Semibold;
	font-weight: 600;
}

.contact_image {
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	transform: translateY(-26%);
	width: 71.2vw;
	height: 49.733333333333334vw;
	z-index: 1;
}

.contact_title {
	margin-top: 8.4vw;
	font-weight: 600;
	margin-bottom: 2.8458666666666668vw;
	font-family: PingFangSC-Semibold;
	font-size: 5.333333333333333vw;
	color: #47087F;
	letter-spacing: 0;
	text-align: center;
}

.contact_body_title {
	text-align: center;
	padding: 7vw 0 4vw 0;
	font-family: PingFangSC-Light;
	font-size: 4.533333333333333vw;
	color: #000000;
	letter-spacing: 0;
	line-height: 7.733333333333333vw;
}

.contact_body {
	width: 82.93333333333334vw;
	height: 34.666666666666664vw;
	margin: 0 auto;
	background: url('data:image/png;base64,') no-repeat;
	background-size: cover;
}

.contact_body_botton {
	display: block;
	margin: auto;
	border-radius: 6vw;
	width: 66.66666666666667vw;
	height: 10.666666666666666vw;
	line-height: 10.666666666666666vw;
	font-family: PingFangSC-Semibold;
	font-size: 5.333333333333333vw;
	color: #FFFFFF;
	letter-spacing: 0;
	text-align: center;
	background: linear-gradient(-134deg, #3023AE 0%, #C86DD7 100%);
	box-shadow: 0 0 24px 0 rgba(62, 9, 143, 0.50);
}

/* End服务号弹窗 */