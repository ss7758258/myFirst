@import "/template/choice/choice.wxss";
@import "/wxParse/wxParse.wxss";
page{
	font-family: 'PingFangSC';
	background: #ffffff;
	color: #000;
}
view{
	overflow: initial;
	word-break: initial;
}
swiper{
	height: calc(100% - 260rpx);
}
swiper-item{
	display: flex;
	flex-direction: column;
}
scroll-view{
	height: 100%;
	display: flex;
	flex-direction: column;
	background:#FAFAFC;
}
.page{
	height: 100%;
	display: flex;
	flex-direction: column;
}
.c-panel{
	height:160rpx;
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.c-desc{
	margin-left: 40rpx;
}
.c-img{
	position: relative;
	width: 120rpx;
	height: 120rpx;
	margin-right: 40rpx;
}
.c-img>image{
	width: 120rpx;
	height: 120rpx;
}
.c-img>.switch{
	position: absolute;
	width: 32rpx;
	height: 32rpx;
	top: 0;
	right: 0;
}
.c-name{
	font-size:36rpx;
	font-weight:600;
	color:rgba(46,52,155,1);
	line-height:50rpx;
}
.c-time{
	font-size:24rpx;
	font-weight:400;
	color:rgba(128,128,166,1);
	line-height:34rpx;
}

/* 轮播指示点 */
.s-dots{
	position: relative;
	height: 100rpx;
	display: flex;
	font-size:28rpx;
	font-weight:600;
	color:rgba(128,128,166,1);
	align-items: center;
	justify-content: space-around;
	box-shadow:0 2rpx 0 0 rgba(247,247,247,1);
	transition: all .3s ease-in-out;
	z-index:2;
}
.s-dots>.current{
	color:rgba(38,35,70,1);
	font-size:32rpx;
}
.s-slider{
	position: absolute;
	bottom: 10rpx;
	left: 0;
	width: 33.333333333333%;
	display: flex;
	justify-content: center;
	transition: all .3s ease-in-out;
}
.s-anm-0{
	left: 0;
}
.s-anm-1{
	left: 33.333333333333%;
}
.s-anm-2{
	left: 66.666666666666%;
}
.s-slider>view{
	width: 50rpx;
	height: 8rpx;
	background:rgba(255,93,124,1);
	border-radius:12rpx;
}

/* 今日运势 */
.d-t-panel{
	background: #ffffff;
	padding-top: 40rpx;
}
.d-t-title{
	width: 670rpx;
	margin: 0 auto;
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.d-t-year{
	position: relative;
	font-size:36rpx;
	font-weight:600;
	color:rgba(46,52,155,1);
}
.d-t-year>view{
	position: absolute;
	font-size:40rpx;
	font-weight:600;
	color:rgba(46,52,155,1);
	top: 10rpx;
	left: 3rpx;
	opacity: .1;
}
.d-t-month{
	width: 48rpx;
	height: 54rpx;
	position: relative;
	display: flex;
	justify-content: center;
	align-items: center;
}
.d-t-month>image{
	position: absolute;
	width: 48rpx;
	height: 54rpx;
	top: 0;
	left: 0;
	z-index: 0;
}
.d-t-month>view{
	margin-top: 8rpx;
	font-size:24rpx;
	font-weight:600;
	color:rgba(38,35,70,1);
	z-index: 1;
}


.d-t-list{
	display: flex;
	justify-content: space-around;
	align-items: center;
	text-align: center;
}
.d-t-list>view{
	width:50rpx;
	height:50rpx;
	background:rgba(244,243,249,1);
	font-size:24rpx;
	font-weight:400;
	color:rgba(173,174,201,1);
	line-height:50rpx;
	border-radius: 50%;
}
.d-t-list>view.current{
	width:70rpx;
	height:70rpx;
	background:linear-gradient(270deg,rgba(255,92,173,1) 0%,rgba(255,93,124,1) 100%);
	box-shadow:0 4rpx 16rpx 0 rgba(255,93,124,0.4);
	line-height:70rpx;
	font-size:32rpx;
	font-weight:600;
	color:rgba(255,255,255,1);
}

/* 运势内容 */
.d-content{
	flex: 1;
	background: #FAFAFC;
	border-radius:24rpx 24rpx 0 0;
}
.d-c-tip{
	display: flex;
	justify-content: space-evenly;
	align-items: center;
	flex-wrap: wrap;
}
.d-c-tip>view{
	width: 330rpx;
	height: 160rpx;
	background:rgba(255,255,255,1);
	border-radius:24rpx;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
}
.d-c-tip>view:nth-child(n + 3){
	margin-top: 30rpx;
}
.d-c-tip image{
	width: 18rpx;
	height: 10rpx;
}
.d-c-tip>view>view{
	font-size:26rpx;
	font-weight:400;
	color:rgba(128,128,166,1);
}
.d-c-tip>view>.d-c-title{
	font-size:36rpx;
	font-weight:600;
	color:rgba(38,35,70,1);
}
.ft{
	font-size: 32rpx !important;
}


/* 基础内容 */
.d-c-tab{
	position: relative;
	width: 710rpx;
	margin: 0 auto;
	display: flex;
	justify-content: space-between;
	align-items: center;
	text-align: center;
	font-size:26rpx;
	font-weight:600;
	color:rgba(173,174,201,1);
	background: #FFF;
	border-radius:24rpx 24rpx 0 0;
}
.d-c-tab>view{
	flex: 1;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	background: #FFF;
	position: relative;
}
.d-c-tab>view:first-child{
	border-radius:24rpx 0 0 0;
}
.d-c-tab>view:nth-child(4){
	border-radius: 0 24rpx 0 0;
}
.border{
	position: absolute;
	top: 0;
	bottom: 0;
	right: -1rpx;
	margin: auto;
	width:2rpx;
	height:48rpx;
	background:rgba(247,247,247,1);
	border-radius:1rpx;
	z-index: 2;
}
.d-c-tab>view.current{
	background: #FFF3F3;
	color:#262346;
}
.d-c-tab image{
	width: 32rpx;
	height: 32rpx;
}
.d-c-text{
	width:28rpx;
	height:28rpx;
	border:2rpx solid rgba(38,35,70,1);
	border-radius: 50%;
	font-size:22rpx;
	font-weight:600;
	color:rgba(38,35,70,1);
	line-height:30rpx;
	text-align: center;
	overflow: hidden;
}
.d-c-tab>view.d-c-bottom{
	flex: auto;
	position: absolute;
	width:670rpx;
	height:2rpx;
	bottom: -2rpx;
	background:rgba(247,247,247,1);
	z-index: 2;
}
/* 详情描述 */
.d-c-desc{
	position: relative;
	width: 710rpx;
	margin: 0 auto;
	padding: 30rpx 40rpx 10rpx 40rpx;
	box-sizing: border-box;
	background: #ffffff;
	font-size:26rpx;
	font-weight:400;
	color:rgba(128,128,166,1);
	line-height:40rpx;
	transition: all .3s ease-in-out;
	min-height: 204rpx;
	overflow: hidden;
	display: flex;
	flex-direction: column;
}
.d-c-desc>text{
	line-height: 30rpx;
}
.more{
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 3;
	overflow: hidden;
}
.flex{
	flex: 1;
}
.more-text{
	width: 660rpx;
	height: 30rpx;
	text-align: right;
	font-size:20rpx;
	font-weight:400;
	color:rgba(173,174,201,1);
	line-height:28rpx;
	display: flex;
	justify-content: flex-end;
	align-items: center;
}
.more-text>image{
	margin-left: 10rpx;
	width: 20rpx;
}
.more-text .current{
	transform: rotate(180deg);
}


/* 运势概况 */
.d-c-score{
	width:710rpx;
	height:264rpx;
	background:rgba(255,255,255,1);
	border-radius:24rpx;
	margin: 0 auto;
}
.d-c-s-title{
	padding: 40rpx 0 0 30rpx;
	font-size:32rpx;
	font-weight:600;
	color:rgba(38,35,70,1);
	line-height:44rpx;
}
.d-c-s-content{
	width: calc(100% - 60rpx);
	margin: 0 auto;
	display: flex;
	justify-content: space-around;
	align-items: center;
	flex-wrap: wrap;
}
.d-c-s-content>view{
	width: 50%;
	display: flex;
	justify-content: space-between;
	align-items: center;
	box-sizing: border-box;
	font-size:28rpx;
	font-weight:400;
	color:rgba(45,52,158,1);
	margin-top: 30rpx;
}
.d-c-s-content>view:nth-child(1)>text,.d-c-s-content>view:nth-child(3)>text{
	margin-right: 20rpx;
}
.d-c-s-content>view:nth-child(2n){
	padding-left: 20rpx;
}
.d-c-s-progress{
	position: relative;
	flex: 1;
	background: #FFD9D9;
	height:8rpx;
	border-radius:4rpx;
	margin-left: 20rpx;
	margin-right: 20rpx;
}
.d-c-s-progress image{
	width:26rpx;
	height:26rpx;
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	margin: auto;
	transition: all 1s linear;
}
.d-c-s-progress>view{
	width: 0%;
	height:8rpx;
	border-radius:4rpx;
	transition: all 1s linear;
	background:linear-gradient(270deg,rgba(255,92,173,1) 0%,rgba(255,93,124,1) 100%);
}
.d-c-s-content>view>text{
	font-size:26rpx;
	font-weight:400;
	color:rgba(173,174,201,1);
}


/* 运势详情 */
.luck-detail{
	width: 690rpx;
	/* height: 220rpx; */
	min-height: 100rpx;
	margin: 0 auto;
}
.luck-panel{
	position: relative;
	width: 650rpx;
	/* height: 220rpx; */
	background:rgba(255,255,255,1);
	border-radius:24rpx;
	margin-left: 40rpx;
}
.luck-icon{
	position: absolute;
	width: 80rpx;
	height: 80rpx;
	box-shadow:0 4rpx 16rpx 0 rgba(38,35,70,0.1);
	border-radius:24rpx;
	top: 20rpx;
	left: -40rpx;
}
.luck-title{
	padding: 20rpx 30rpx 20rpx 60rpx;
	font-size:32rpx;
	font-weight:600;
	line-height:44rpx;
	color:rgba(38,35,70,1);
}
.luck-desc{
	padding: 0 30rpx 20rpx 60rpx;
	font-size:26rpx;
	font-weight:400;
	color:rgba(128,128,166,1);
	line-height:36rpx;
}


/* 本周运势 */
.line{
	width:30rpx;
	height:6rpx;
	background:rgba(255,216,23,1);
	border-radius:5rpx;
}
.d-c-tip>view:nth-child(2)>.line{
	background:rgba(26,244,255,1);
}
.d-c-tip>view:nth-child(3)>.line{
	background:rgba(121,102,255,1);
}
.d-c-tip>view:nth-child(4)>.line{
	background:rgba(255,143,8,1);
}

.w-panel{
	width: 100%;
	background: #ffffff;
}
.w-panel image{
	width: 29rpx;
	height: 29rpx;
}
.w-title{
	padding: 40rpx;
	display: flex;
	align-items: center;
}
.w-title>view{
	width:40rpx;
	height:88rpx;
	font-size:32rpx;
	font-weight:600;
	color:rgba(38,35,70,1);
	line-height:44rpx;
	text-align: center;
}
.w-title>view.w-date{
	flex: 1;
	margin-left: 22rpx;
	width: auto;
	position: relative;
	text-align: left;
	display: flex;
	flex-direction: column;
	font-size:36rpx;
	font-weight:600;
	color:rgba(46,52,155,1);
	line-height:50rpx;
}
.w-title>view.w-date>text:nth-child(2){
	font-size:28rpx;
	font-weight:600;
	color:rgba(128,128,166,1);
	line-height:40rpx;
}
.w-date .tip-op{
	position: absolute;
	top: 16rpx;
	right: 0;
	width: 288rpx;
	height: 60rpx;
}
.text-panel{
	padding: 0 38rpx;
	font-size:28rpx;
	font-weight:600;
	color:rgba(46,52,155,1);
	line-height:40rpx;
	display: flex;
}
.text-panel>image{
	margin-top: 6rpx;
}
.red{
	color: #FF5D7C;
}
.text-panel:last-child{
	padding-bottom:  40rpx ;
}
.text-panel>view{
	margin-left: 10rpx;
}