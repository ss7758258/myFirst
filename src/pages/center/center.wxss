@import "/template/tabbar/tabbar.wxss";
page{
    font-family:PingFangSC;
}
.panel{
    background: #FFFFFF;
    height: calc(100% - 64px);
}
.photo{
    height: 250rpx;
    background: url(data:image/png;base64,) center / cover no-repeat;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: flex-end;
}
.photo > image{
    width: 170rpx;
    height: 170rpx;
    border-radius: 50%;
    overflow: hidden;
}
.text{
    display: flex;
    align-items: center;
    justify-content: center;
    font-size:36rpx;
    font-weight:600;
    color:rgba(38,35,70,1);
    line-height:50rpx;
    text-align: center;
}
.flex-panek{
    display: flex;
    justify-content: center;
    align-items: center;
}
.text-img{
    width:28rpx;
    height:28rpx;
    /* background:rgba(255,93,124,1); */
    border-radius: 50%;
    margin-left: 10rpx;
}
.edit-panel{
    display: flex;
    justify-content: center;
    align-items: center;
}
.edit-panel>view{
    width:210rpx;
    height:60rpx;
    border-radius:30rpx;
    border:2rpx solid rgba(244,243,249,1);
}
.edit-panel text{
    font-size:24rpx;
    font-weight:400;
    color:rgba(128,128,166,1);
    margin-left: 10rpx;
}
.edit-panel image{
    width:28rpx;
    height:32rpx;
}
.flex{
    display: flex;
    width: 690rpx;
    margin: 0 auto;
    box-shadow: inset 0 -1px 0 0 rgba(0,0,0,0.10);
}
.flex>view{
    display: flex;
    align-items: center;
}
.d-center{
    flex: 1;
    display: flex;
    justify-content: space-between;
    align-items: center;
    height:40rpx;
    font-size:28rpx;
    font-weight:600;
    color:rgba(38,35,70,1);
    height:108rpx;
}
.left,.right{
    width: 50rpx;
}
.left{
    margin-right: 20rpx;
}
.right{
    justify-content: flex-end;
}
.notice_icon{
    width: 48rpx;
    height: 48rpx;
}
.right>.notice_icon{
    width:16rpx;
    height:26rpx;
}
.switchOn{
    position: absolute;
    top: 0;
    bottom: 0;
    margin: auto;
    width: 80%;
    height: 7.2vw;
    background:rgba(146, 98, 251, 1);
    border-radius:3.6vw;
    transition: all .3s linear;
}
.switchOn.off{
    background:rgba(146, 98, 251, .3);
}
.switchOn>view{
    position: relative;
    width: 100%;
    height: 100%;
    border-radius: 3.6vw;
    overflow: hidden;
}
.switchOn>view>view{
    position: absolute;
    top: 0;
    bottom: 0;
    margin: auto;
    left: calc(100% - 7vw);
    width: 6.8vw;
    height: 6.8vw;
    background: #FFFFFF;
    border-radius:50%; 
    transition: all .2s ease-in-out;
    box-shadow:0 0 3px rgba(0,0,0,.1);
}
.switchOn.off>view>view{
    left: 0.2vw;
    right: auto;
}
.card_star{
    /* float: right; */
    font-size:28rpx;
    font-weight:400;
    color:rgba(128,128,166,1);
}