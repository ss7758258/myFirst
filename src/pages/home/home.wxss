/* 对话框 */
.dialog-mask{
	position: fixed;
	width: 100%;
	height: 100%;
	background:rgba(0,0,0,.7);
	z-index: 9;
}
.dialog-panel{
	position: fixed;
	width: 620rpx;
	height: 900rpx;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	display: flex;
	align-items: center;
}
.dialog{
	width: 100%;
	position: relative;
	text-align: center;
	background:rgba(255,255,255,1);
	border-radius:8rpx;
}
.dialog-title{
	width: 550rpx;
	height: 130rpx;
	margin: 0 auto;
	font-size:40rpx;
	font-family:PingFangSC;
	font-weight: 600;
	color:rgba(71,71,71,1);
	line-height:130rpx;
	border-bottom: 1rpx dashed rgba(146,98,251,1);
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.dialog-icon{
	width: 100%;
	height: 197rpx;
}
.dialog-icon>image{
	display: block;
	width: auto;
	height: 197rpx;
}
.dialog-content{
	padding: 0 80rpx;
	font-size:36rpx;
	font-family:PingFangSC;
	color:rgba(71,71,71,1);
	line-height:50rpx;
}
.dialog-btns{
	padding: 0 44rpx;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
}
.dialog-btns>button{
	width:128rpx;
	height:45rpx;
	font-size:32rpx;
	font-family:PingFangSC;
	color:rgba(191,191,191,1);
	line-height:45rpx;
}
.dialog-btns>button.dialog-sure{
	width:500rpx;
	height:100rpx;
	background:linear-gradient(180deg,rgba(250,217,97,1),rgba(247,107,28,1));
	border-radius:50rpx;
	color: #fff;
	font-size:50rpx;
	line-height:100rpx;
	transition: all .3s ease-in-out;
}
.dialog-btns>button.dialog-cancel{
	width: 100%;
	margin: 30rpx auto 30rpx auto;
}
.dialog-close{
	width: 40rpx;
	height: 40rpx;
	background:rgba(216,216,216,.7);
	position: absolute;
	border-radius: 50%;
	top: 21rpx;
	left: 15rpx;
}
.dialog-close image{
	width: 40rpx;
	height: 40rpx;
	border-radius: 50%;
}
.btn-current{
	transform: scale(.9);
}
