page{
	background: #fff;
}
.birth{
	margin-left: 15rpx;
	margin-top: 5rpx;
	width: 20rpx;
	height: 26rpx;
}
page button {
	width: 100%;
	height: 100%;
	margin: 0;
	padding: 0;
	box-sizing: initial;
	overflow: initial;
	background: inherit;
	line-height : normal;
	border-radius: inherit;
	font-size: 0;
}
button::after{
	content: '';
	box-shadow: initial;
	border: none;
}
.pt65{
	padding-top: 65rpx;
}
.monitor-panel{
	width: 100%;
	min-height: calc(100% - 64px);
	height: calc(100% - 64px);
	background: #fff;
	transform: scale(0);
	opacity: 0;
	transition: all .3s ease-in-out;
}
.monitor-panel.current{
	opacity: 1;
	transform: scale(1);
}
/* iPhoneX的适配 */
.iPX .monitor-panel{
	min-height: calc(100% - 89px);
	height: calc(100% - 89px);
}
.star-panel{
