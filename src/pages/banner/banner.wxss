
.listBanner{
    height: calc(100% - 2.9962546816479403vh - 6.6vh);
}
.el-dialo{
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background:rgba(0,0,0,.7);
    z-index: 101;
    transform: scale(0);
}
.el-dialo-panel{
    position: fixed;
    width: 570rpx;
    height: 240rpx;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    background: #ffffff;
    border-radius:6rpx;
}
.el-dialo-content{
    height:44rpx;
    font-size:32rpx;
    font-family:PingFangSC;
    font-weight: 600;
    color:rgba(51,51,51,1);
    line-height:44rpx;
    text-align: center;
}
.el-dialo-sure{
    width:270rpx;
    height:80rpx;
    background:rgba(146,98,251,1);
    border-radius:6rpx;
    font-size:36rpx;
    font-family:PingFangSC;
    font-weight: 600;
    color:rgba(255,255,255,1);
    line-height:80rpx;
}
.el-dialo.show{
    transform: scale(1);
}
ad{
    position: fixed;
    bottom: 0;
    z-index: 99;
}