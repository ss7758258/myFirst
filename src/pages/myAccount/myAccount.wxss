
.pannerl-panel{
	padding: 0 0 4vw;
	width: 100%;
	background: #fff;
}
.pannel-wrapper {
	position: relative;
	padding: 0 30rpx;
}
.balance-panel{
	padding: 0;
	width: 100%;
	background:rgba(146,98,251,1);
}
.mb30 {
	margin-bottom: 30rpx
}

.mb40 {
	margin-bottom: 40rpx
}

.mb60 {
	margin-bottom: 60rpx
}

.mb160 {
	margin-bottom: 160rpx
}

.balance-info {
	position: relative;
	color: #fff;
	box-sizing: border-box;
	border-radius: 12rpx;
	overflow: hidden;
	padding: 70rpx 0;
	width: 100%;
	height: 22.488755622188904vh;
}

.balance-info .item {
	display: block;
	text-align: center;
	font-size:28rpx;
	font-family:'PingFangSC-Regular';
	color:rgba(255,255,255,1);
	line-height:40rpx;
}

.balance-info .title {
	font-size: 18px;
	margin-bottom: 40rpx;
}

.balance-info .balance {
	font-weight: 700;
	margin-top: 24rpx;
	font-size:64rpx;
	font-family:'PingFangSC-Medium';
	color:rgba(255,255,255,1);
	line-height:90rpx;
}

.balance-info .desc {
	position: relative;
	display: inline-block;
	font-size: 12px;
	padding-left: 22px;
	line-height: 20px;
	margin-right: 15rpx;
}

.balance-info .desc::before {
	position: absolute;
	left: 0;
	top: 2px;
	width: 20px;
	height: 20px;
	background-size: contain;
	content: "";
	background: no-repeat center url('data:image/svg+xml;base64,');
}

.star-title {
	font-size: 18px;
	color: #1A0C35;
	font-weight: 700;
}

.star-list {
	display: flex;
	flex-wrap: wrap;
	align-items: flex-start;
	align-content: flex-start;
	justify-content: space-between;
}

.star-list .item {
	flex: 0 0 210rpx;
	text-align: center;
	width: 210rpx;
	height: 130rpx;
	padding: 15rpx 0;
	box-sizing: border-box;
	line-height: 1.8;
	border-radius: 8rpx;
	border: 1px solid #E6E6E6
}

.star-list .item:nth-child(-n+3) {
	margin-bottom: 30rpx
}

.star-list .item .total {
	font-size: 16px;
	color: #666;
	font-weight: 600
}

.star-list .item .price {
	font-size: 14px;
	color: #999;
}

.star-list .item.active {
	border: none;
	background:linear-gradient(to left,rgba(174,69,255,1),rgba(146,98,251,1));
	box-shadow: 0 6rpx 12rpx rgba(155, 70, 216, .4)
}

.star-list .item.active .total, .star-list .item.active .price {
	color: #fff;
}

.pay-btn {
	color: #fff;
	background:linear-gradient(to left,rgba(174,69,255,1),rgba(146,98,251,1));
}
button.pay-btn.disabled {
	color: #fff;
	background: #cccccc;
}
button.pay-btn[disabled] {
	color: #fff;
	opacity: 0.65;
}

.pay-btn-active {
	background: linear-gradient(to bottom, rgb(157, 36, 250), rgb(129, 72, 253))
}