.navigtion_panel{
    padding-top: 20px;
}
.iPhoneX.navigtion_panel{
    padding-top: 45px;
}
.navigtion_nav{
    height: 44px;
    text-align: center;
    position: relative;
}
.nav_title{
    display: inline-block;
    line-height: 43px;
    font-family: 'PingFangSC';
    font-size: 4.533333333333333vw;
    letter-spacing: -0.10933333333333332vw;
    height: 44px;
    width: 70vw;
	overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    /* font-weight: 600; */
}
.nav_state{
    position: absolute;
    width: 8vw;
    height: 8vw;
    border-radius: 50%;
    line-height:44px;
    top:0;
    bottom: 0;
    margin: auto;
    left:4.4vw;
    overflow:hidden;
    box-shadow: 0 0 3px rgba(0,0,0,.1);
}
.nav_state.hide{
    top:-1000px;
}
.nav_state.default_icon{
    background: url(data:image/svg+xml;base64,) center / contain no-repeat;
}
.back_img{
    border-radius: 0;
    box-shadow: none;
    left:0;
    background: url(data:image/svg+xml;base64,) center / contain no-repeat;
}
.home_img{
    /* width: 6vw;
    height: 6vw; */
    border-radius: 0;
    /* left:6.4vw; */
    left:0;
    box-shadow: none;
    background: url(data:image/svg+xml;base64,) center / contain no-repeat;
    /* background: url(data:image/svg+xml;base64,) center / contain no-repeat; */
}

.home_img{
    width: 6vw;
    height: 6vw;
    border-radius: 0;
    left:6.4vw;
    box-shadow: none;
    background: url(data:image/png;base64,) center / contain no-repeat;
    /* background: url(data:image/svg+xml;base64,) center / contain no-repeat; */
}