

@import "/template/choice/choice.wxss";
@import "/template/star-desc/star-desc.wxss";
@import "/template/tabbar/tabbar.wxss";

page button {
	width: 100%;
	height: 100%;
	margin: 0;
	padding: 0;
	box-sizing: initial;
	overflow: initial;
	background: inherit;
	line-height : normal;
	border-radius: inherit;
	font-size: 0;
}
button::after{
	content: '';
	box-shadow: initial;
	border: none;
}
.pt65{
	padding-top: 65rpx;
}
.monitor-panel{
	width: 100%;
	min-height: calc(100% - 64px);
	height: calc(100% - 64px);
	background: #fff;
}
/* iPhoneX的适配 */
.iPX .monitor-panel{
	min-height: calc(100% - 89px);
	height: calc(100% - 89px);
}
.star-panel{
