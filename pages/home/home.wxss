

@import "/template/choice/choice.wxss";
@import "/template/star-desc/star-desc.wxss";
@import "/template/notice/notice.wxss";

page button {
	width: 100%;
	height: 100%;
	margin: 0;
	padding: 0;
	box-sizing: initial;
	overflow: initial;
	background: inherit;
	line-height : normal;
	border-radius: inherit;
	font-size: 0;
}
botton::after{
	content: '';
	box-shadow: initial;
	border: none;
}
.pt65{
	padding-top: 65rpx;
}
.monitor-panel{
	width: 100%;
	min-height: calc(100% - 64px);
	height: calc(100% - 64px);
	background: #fff;
}
/* iPhoneX的适配 */
.iPX .monitor-panel{
	min-height: calc(100% - 89px);
	height: calc(100% - 89px);
}
.star-panel{
	position: relative;
	width: 710rpx;
	height: 630rpx;
	background: #FFFFFF;
	border-radius: 12rpx;
	margin: -10rpx auto 0 auto;
	box-shadow:0px 8rpx 16rpx 0px rgba(0,0,0,0.1);
}
.star-mask{
	width: 710rpx;
	height: 20rpx;
	border-radius: 10rpx 10rpx 0 0;
	margin: 0 auto;
	background-image: linear-gradient(-27deg, #C86DD7 0%, #3023AE 100%);
}
.star-position{
	position: relative;
	width: 100%;
	height: 260rpx;
}
/* 切换星星按钮 */
.star-xz{
	position: absolute;
	left : 16.5rpx;
	top: -70rpx;
	width: 300rpx;
	height: 300rpx;
}
.star-xz-switch{
	position: absolute;
	left : 16.5rpx;
	bottom: 30rpx;
	width: 50rpx;
	height: 50rpx;
}
.star-xz-brand{
	position: absolute;
	right : 27.75rpx;
	top: -60rpx;
	width: 368rpx;
	height: 227rpx;
}
.star-btn-tip{
	position: absolute;
	right : 30rpx;
	bottom: 35rpx;
	width: 30rpx;
	height:30rpx;
}
.star-xz-brand-btn{
	position: relative;
}
.star-btn-panel{
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	height: 97rpx;
	width: 100%;
	text-align: center;
	font-size: 55rpx;
	color: #FFFFFF;
	letter-spacing: 3.67rpx;
}
.star-btn-panel>text{
	margin-top:-9rpx;
	display: block;
}
.star-btn-panel>.star-btn-time{
	font-family: 'PingFangSC-Medium';
	font-size: 24rpx;
	color: #FFFFFF;
	letter-spacing: 0;
	margin-top:9rpx;
}
.star-xz-img,.star-xz-brand-img{
	width: 100%;
	height: 100%;
}
/* 星座指标信息 */
.star-info{
	display: flex;
	width: 650rpx;
	margin: 0 auto;
	padding-bottom: 40rpx;
	font-family: PingFangSC-Regular;
	font-size: 26rpx;
	color: #3718A5;
	letter-spacing: 0;
	text-align: center;
	border-bottom: 1rpx #6041CD dashed;
}
.star-info>view{
	flex: 1;
}
.star-info>view .star-text{
	font-family: PingFangSC-Medium;
	font-size: 31rpx;
	color: #3718A5;
	letter-spacing: 0;
	margin-top: 14rpx;
	text-align: center;
	font-weight: 600;
}

/* End星座指标信息 */

/* 星座提醒 */
.star-tip{
	display: flex;
	position: relative;
	align-items: center;
	justify-content: space-between;
	width: 650rpx;
	margin: 0 auto;
	padding: 40rpx 0;
	border-bottom: 1rpx #6041CD dashed;
}

.star-tip-nav{
	position: absolute;
	width: 132rpx;
	height: 138rpx;
	right: 0;
	animation: rotateNav 1s 0s linear infinite;
}
@keyframes rotateNav{
	0%,100%{
		transform: rotate(5deg);
	}
	50%{
		transform: rotate(-5deg);
	}
}
.star-tip-nav>navigator{
	width: 100%;
	height: 100%;
}
.star-tip-gz{
	width: 100%;
	height: 100%;
}
.star-warn{
	width: 44rpx;
	height: 44rpx;
	vertical-align: middle;
}
.star-tip-text{
	flex: 1;
	overflow: hidden;
	text-overflow:ellipsis;
    white-space:nowrap;
	font-family: PingFangSC;
	font-size: 27rpx;
	color: #6041CD;
	letter-spacing: 0;
	text-align: justify;
}
.star-tip-notice{
	width: 200rpx;
	height: 62rpx;
	font-family: PingFangSC-Medium;
	font-size: 29rpx;
	color: #FFFFFF;
	letter-spacing: 0;
	vertical-align: middle;
	text-align: center;
	line-height: 62rpx;
	background: #6041CD;
	box-shadow: 0 2px 4px 0 rgba(96,65,205,.5);
	border-radius: 10rpx;
}
.star-tip-notice-nav{
	width: 132rpx;
}
/* End星座提醒 */

/* 更多运势 */
.star-more{
	width: 650rpx;
	margin: 0 auto;
	/* height: 40rpx; */
	padding: 30rpx 0;
	display: flex;
	justify-content: space-between;
	color: #6041CD;
}

.star-more>view:first-child{
	position: relative;
	width: 132rpx;
	height: 138rpx;
}
/* 关注页面样式 */
.gz{
	position: absolute;
	top: 0;
	left: 0;
	width: 132rpx;
	height: 138rpx;
	transform: translateY(-50rpx);
}
.star-more-text{
	display: flex;
	align-items:center;
	font-family: PingFangSC-Medium;
	font-size: 29rpx;
	letter-spacing: 0;
	text-align: justify;
	color: #6041CD;
}
.star-trigon{
	display: block;
	width: 0;
	height: 0;
	margin: 0 20rpx 0 10rpx;
    border-width:14rpx 0 14rpx 14rpx;
    border-style:solid;
    border-color:rgba(0,0,0,0) rgba(0,0,0,0) rgba(0,0,0,0) #6041CD;
}
/* End更多运势 */

/* 配对 */
.star-pair-img,.star-banner-img{
	width: 710rpx;
	height: 280rpx;
	margin: 30rpx auto 0;
	background-image: linear-gradient(-270deg, #FF93BA 0%, #FFDFE9 100%);
	border-radius: 10rpx;
	box-shadow:0px 8rpx 16rpx 0px rgba(254,148,186,0.4);
}
.star-banner-img{
	box-shadow:0px 8rpx 16rpx 0px rgba(121,218,255,0.4);
}
.star-text-panel{
	position: absolute;
	width: 710rpx;
	height: 280rpx;
	top: 30rpx;
	left: 0;
	right: 0;
	margin: auto;
	text-align: left;
	border-radius:10rpx;
}
.star-text-panel>text{
	display: block;
	text-indent: 40rpx;
}
.star-text-panel>text:nth-child(1){
	font-size:72rpx;
	margin-top: 64rpx;
	font-weight: 500;
	font-family:PingFangSC-Medium;
	color:rgba(255,255,255,1);
	line-height:100rpx;
}
.star-text-panel>text:nth-child(2){
	font-size:36rpx;
	font-family:PingFangSC-Regular;
	color:rgba(255,255,255,1);
	line-height:50rpx;
}
.star-unclaimed{
	position: absolute;
	top: 0;
	left: 40rpx;
	width: 128rpx;
	height: 60rpx;
	font-size:28rpx;
	font-family:PingFangSC-Regular;
	color:rgba(255,255,255,1);
	line-height:40rpx;
	text-align: center;
}
.star-unclaimed image,.star-unclaimed>view{
	position: relative;
	width: 100%;
	height: 100%;
}
.star-unclaimed text{
	position: absolute;
	width: 100%;
	height: 40rpx;
	font-size:28rpx;
	line-height: 35rpx;
	bottom: 0;
	left: 0;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
/* End配对 */
.panel:last-child{
	padding-bottom: 20rpx;
}

/* 加载动画 */
.loader_panel{
	position: fixed;
	height:100px;
	width:100px;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	z-index: 1;
}
#caseBlanche {
	height : 100px;
	width : 100px;
	padding-top : 10px;
	float : left;
	position : relative;
}
#rond {
	height  : 60px;
	width : 60px;
	border : 1px solid #fff;
	border-radius : 50%;
	position :absolute;
	top : 0;
	left : 0;
	right: 0;
	bottom: 0;
	margin: auto;
	animation : rond 2s infinite;
	-webkit-animation : rond 2s infinite;
}
#caseBlanche #load {
	width: 60px;
	height: 16px;
	font-size: 12px;
	color : #fff;
	text-align : center;
	position : absolute;
	top : 0;
	left :0;
	right: 0;
	bottom: 0;
	margin: auto;
}
#test {
	height : 10px;
	width : 10px;
	position : absolute;
	background-color : #fff;
	border-radius : 50%;
	top : 0px;
	left : 8px;
}
.loader_mask{
	position: fixed;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	background: rgba(0,0,0,.5);
	z-index: 1;
}

@keyframes rond {
	0% {transform : rotate(0deg);}
	100% {transform : rotate(360deg);}
}

@-webkit-keyframes rond {
	0% {-webkit-transform : rotate(0deg);}
	100% {-webkit-transform : rotate(360deg);}
}

.more_text {
	position: absolute;
	width: 126rpx;
	height: 40rpx;
	top: 0;
	right: 0;
	text-align: center;
	font-size: 24rpx;
	font-family: 'PingFangSC-Regular';
	color: rgba(255, 255, 255, 1);
	line-height: 40rpx;
	background: rgba(255, 38, 38, 0.2);
	border-radius: 0 10rpx 0 12rpx;
}

/* 服务号弹窗 */

.contact_follow_mask {
	position: fixed;
	width: 100%;
	height: 100%;
	top: -100%;
	left: 0;
	background: rgba(0, 0, 0, .6);
	z-index: 99;
}

.contact_follow_mask.show {
	top: 0;
}

.contact_follow {
	position: absolute;
	top: -100%;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	width: 88vw;
	height: 93.33333333333333vw;
	transform: translateY(54px);
	background: #fff;
	transition: all .15s ease-in-out;
	z-index: 100;
}

.contact_follow.show {
	top: 0;
}

.contact_panel {
	position: relative;
	width: 100%;
	height: 100%;
}

.contact_head_panel {
	width: 100%;
	height: 37.333333333333336vw;
	overflow: hidden;
}

.contact_head_panel_2 {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 37.333333333333336vw;
}

.contact_head_panel_2 .contact_head {
	width: 100%;
	margin: auto;
	background: rgba(0, 0, 0, 0);
}

.contact_head {
	position: relative;
	width: 110%;
	margin-left: -5%;
	height: 37.333333333333336vw;
	border-radius: 0 0 50vw 50vw / 0 0 12vw 12vw;
	background: linear-gradient(0deg, #9262FB 0%, #bfa3fd 100%);
}

.contact_head_text {
	position: absolute;
	display: block;
	width: 65.33333333333333vw;
	height: 12.8vw;
	left: 0;
	right: 0;
	bottom: 0;
	top: 0;
	margin: auto;
	z-index: 1;
	font-family: PingFangSC-Regular;
	font-size: 4.533333333333333vw;
	color: #000000;
	letter-spacing: 0;
	text-align: center;
}

.contact_head_text .st {
	font-family: PingFangSC-Semibold;
	font-weight: 600;
}

.contact_image {
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	transform: translateY(-26%);
	width: 71.2vw;
	height: 49.733333333333334vw;
	z-index: 1;
}

.contact_title {
	margin-top: 8.4vw;
	font-weight: 600;
	margin-bottom: 2.8458666666666668vw;
	font-family: PingFangSC-Semibold;
	font-size: 5.333333333333333vw;
	color: #47087F;
	letter-spacing: 0;
	text-align: center;
}

.contact_body_title {
	text-align: center;
	padding: 7vw 0 4vw 0;
	font-family: PingFangSC-Light;
	font-size: 4.533333333333333vw;
	color: #000000;
	letter-spacing: 0;
	line-height: 7.733333333333333vw;
}

.contact_body {
	width: 82.93333333333334vw;
	height: 34.666666666666664vw;
	margin: 0 auto;
	background: url('data:image/png;base64,') no-repeat;
	background-size: cover;
}

.contact_body_botton {
	display: block;
	margin: auto;
	border-radius: 6vw;
	width: 66.66666666666667vw;
	height: 10.666666666666666vw;
	line-height: 10.666666666666666vw;
	font-family: PingFangSC-Semibold;
	font-size: 5.333333333333333vw;
	color: #FFFFFF;
	letter-spacing: 0;
	text-align: center;
	background: linear-gradient(-134deg, #3023AE 0%, #C86DD7 100%);
	box-shadow: 0 0 24px 0 rgba(62, 9, 143, 0.50);
}

/* End服务号弹窗 */
