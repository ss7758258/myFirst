/* 摇签卡片样式 */
.star-card-icon{
	position: absolute;
	width: 180rpx;
	height: 180rpx;
	top: 40rpx;
	bottom: 0;
	margin: auto;
	right: 54rpx;
}
.star-icon-panel{
	position: relative;
	width: 100%;
	height: 100%;
}
.shake_4{
	position: absolute;
	width: 147rpx;
	height: 147rpx;
	top: 12rpx;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
	animation: rotateShake 1s 0s infinite linear;
	transform-origin: center 80%;
	border-radius: 0 0 90rpx 70rpx;
}
.shake_star{
	position: absolute;
	width: 44rpx;
	height: 44rpx;
	top: 60rpx;
	right: 40rpx;
	opacity: 0;
	/* z-index: 5; */
	animation: star_animate 1s 0s infinite linear;
}
.shake_star:nth-child(4){
	width: 28rpx;
	height: 28rpx;
	top: 100rpx;
	left: 0;
	animation: star_animate 1s .1s infinite linear;
}
.shake_star:nth-child(5){
	width: 21rpx;
	height: 21rpx;
	top: auto;
	bottom: 21rpx;
	right: 21rpx;
	animation: star_animate 1s .2s infinite linear;
}
@keyframes star_animate{
	0%,100%{
		opacity: 0;
	}
	15%{
		opacity: 1;
		transform: translateY(-20rpx) translateX(20rpx);
	}
	30%{
		opacity: 1;
		transform: translateY(-50rpx) translateX(0);
	}
	60%{
		opacity: 0;
		transform: translateY(-80rpx) translateX(0);
	}
	61%{
		transform: translateY(0) translateX(0);
	}
}
.star-icon-border{
	position: absolute;
	width: 147rpx;
	height: 147rpx;
	top: 0;
	bottom: 0;
	left: 0;
	right: 6rpx;
	margin: auto;
	border: 8rpx solid #E4B8FC;
	border-radius: 50%;
}
@keyframes rotateShake{
	0%,100%{
		transform: rotate(0);
	}
	15% {
		transform: rotate(-6deg);
	}
	30% {
		transform: rotate(4deg);
	}
	45% {
		transform: rotate(-2deg);
	}
	60%{
		transform: rotate(0);	
	}
}

/* End摇签卡片样式 */


