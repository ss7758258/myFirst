/* 星座提醒 */
.star-tip{
	display: flex;
	position: relative;
	align-items: center;
	justify-content: space-between;
	width: 650rpx;
	margin: 0 auto;
	padding: 36rpx 0;
	/* border-bottom: 1rpx #6041CD dashed; */
}
.collect{
	position: relative;
	width: 132rpx;
}
.star-tip-nav{
	position: absolute;
	width: 132rpx;
	height: 138rpx;
	right: 0;
	animation: rotateNav 1s 0s linear infinite;
}
.collect .star-tip-nav{
	top: -45rpx;
	animation: none;
}
.collect-close{
    position: absolute;
    right: -30rpx;
    top: -48rpx;
    width: 48rpx;
    height: 48rpx;
    background:rgba(255,255,255,1);
    border-radius: 50%;
    text-align: center;
    line-height: 48rpx;
	opacity:0.5;
	border: 1rpx solid rgba(0,0,0,.1);
}
.collect-close image{
    display: inline-block;
    width: 24rpx;
    height: 24rpx;
    margin-top: 12rpx;
}
.star-collect{
	width: 132rpx;
	height: 138rpx;
}
@keyframes rotateNav{
	0%,100%{
		transform: rotate(5deg);
	}
	50%{
		transform: rotate(-5deg);
	}
}
.star-tip-nav>navigator{
	width: 100%;
	height: 100%;
}
.star-tip-gz{
	width: 100%;
	height: 100%;
}
.star-warn{
	width: 44rpx;
	height: 44rpx;
	vertical-align: middle;
}
.star-tip-text{
	flex: 1;
	overflow: hidden;
	text-overflow:ellipsis;
    white-space:nowrap;
	font-family: PingFangSC;
	font-size: 29rpx;
	color: #6041CD;
	letter-spacing: 0;
	text-align: justify;
}
.star-tip-notice{
	width:160rpx;
	height:60rpx;
	font-family: PingFangSC-Medium;
	font-size: 29rpx;
	color: #FFFFFF;
	font-weight: 600;
	letter-spacing: 0;
	vertical-align: middle;
	text-align: center;
	line-height: 60rpx;
	background:linear-gradient(180deg,rgba(196,155,253,1),rgba(146,98,251,1));
	border-radius:31rpx;
	box-shadow: 0 2px 4px 0 rgba(96,65,205,.5);
}
.star-tip-notice-nav{
	width: 132rpx;
}
/* End星座提醒 */

/* 更多运势 */
