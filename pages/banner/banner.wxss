
.listBanner{
    height: calc(100% - 2.9962546816479403vh - 6.6vh);
}
.el-dialo{
    position: fixed;
    width: 270rpx;
    height: 52rpx;
    line-height: 52rpx;
    text-align: center;
    font-size:24rpx;
    font-family:'PingFangSC-Regular';
    color:rgba(255,255,255,1);
    background:rgba(255,193,64,1);
    box-shadow:0px 4rpx 8rpx 0px rgba(255,193,64,0.5);
    border-radius:26rpx;
    z-index: 1;
    left: 0;
    right: 0;
    transform: scale(0);
    transition: all .2s ease-in-out;
    margin: auto;
}
.el-dialo.show{
    transform: scale(1);
}
ad{
    position: fixed;
    bottom: 0;
    z-index: 99;
}