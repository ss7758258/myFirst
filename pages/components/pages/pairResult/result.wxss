
page{
    background: #fff;
}
page button {
	width: 100%;
	height: 100%;
	margin: 0;
	padding: 0;
	box-sizing: initial;
	overflow: initial;
	background: inherit;
	line-height : normal;
	border-radius: inherit;
	font-size: 0;
}
.container image{
    width: 100%;
    height: 100%;
}
.mt90{
    margin-top: 90rpx !important;
}
.mt60{
    margin-top: 60rpx !important;
}
.mt40{
    margin-top: 40rpx !important;
}
.mt20{
    margin-top: 20rpx !important;
}
.pt30{
    padding-top: 30rpx !important;
}
.container{
    position: relative;
    min-height: 100%;
    background: #fff;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}
/* 头部 */
.pair-panel-top{
    position: relative;
    width: 100%;
    flex: 1;
    margin-top: 60rpx;
}
.margin-panel{
    flex: 1;
}
.container .pair-top-bg{
    position: absolute;
    width: 100%;
    height: 109rpx;
    top: 0;
    left: 0;
}
.pair-panel-top .pair-top-pair{
    display: block;
    margin: 0 auto;
    width: 316rpx;
    height: 82rpx;
}

/* 星座卡片 */
.pair-panel-top .pair-card-panel{
    display: flex;
    justify-content: space-around;
    text-align: center;
    z-index: 99;
}
.pair-card-me{
    position: relative;
}
/* 卡片布局 */
.card-panel{
    width: 280rpx;
    height:400rpx;
    position: relative;
    background: #fff;
    border-radius:8rpx;
    box-shadow: 0px 6rpx 12rpx rgba(146,98,251,.2);
}
.card-dom{
    width: 260rpx;
    height: 380rpx;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    border-radius:8rpx;
    border:2rpx solid currentColor;
}
.card-name{
    position: absolute;
    top: 10rpx;
    left: 10rpx;
    width:28rpx;
    height:84rpx;
    font-size:24rpx;
    font-family:PingFangSC-Medium;
    line-height:28rpx;
}
.card-icon{
    position: absolute;
    bottom: 10rpx;
    right: 10rpx;
    height:34rpx;
    font-size:30rpx;
}
.container .card-xz{
    position: absolute;
    width: 210rpx;
    height: 262rpx;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
}
.pair-card-me text{
    display: inline-block;
    width: 210rpx;
    padding: 0 15rpx;
    height: 48rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    border-radius:24rpx;
    font-size:28rpx;
    font-family:'PingFangSC-Medium';
    color:rgba(146,98,251,1);
    line-height:49rpx;
    margin-top: 20rpx;
}
.pair-card-me .pair-switch{
    display: block;
    width: 80rpx;
    height: 80rpx;
    left: 0;
    right: 0;
    bottom: 0rpx;
    transform: translateY(50%);
    margin: auto;
    position: absolute;
}
.pair-panel-top .pair-card-x{
    display: block;
    margin: auto 0;
    width: 60rpx;
    height: 60rpx;
    transform: translateY(-40rpx);
}

/* 圆环 */
.clip-panel{
    width: 100%;
    height: 310rpx;
}
.box{
    width: 280rpx;
    height: 280rpx;
    position: relative;
    background-color: #ccc;
    border-radius: 50%;
    margin: auto;
}
.clip-num{
    position: absolute;
    top: 50%;
    left: 50%;
    background: #fff;
    border-radius: 50%;
    width: 260rpx; 
    height: 260rpx;
    transform: translate(-50%, -50%);
    text-align: center;
    display: flex;
    align-items: center;
    justify-content: center;
}
.clip-num>view{
    font-size:96rpx;
    font-family:PingFangSC-Medium;
    color:rgba(255,87,94,1);
}
.clip-num>view>text{
    display: block;
}
.pair-text{
    font-size:24rpx;
    color:rgba(153,153,153,1);
}
.pair-text-num{
    font-weight: 600;
}
.clip{
    width: 280rpx;
    height: 280rpx;
    position: absolute;
    background: #FF575E;
    border-radius: 50%;
}
.left{
    width: 280rpx;
    height: 280rpx;
    position: absolute;
    border-radius: 50%;
    background:rgb(252, 196, 196);
    clip: rect(0 140rpx 280rpx 0);
    transform: rotate(0deg);
    transition: all 1s linear;
}
.right{
    width: 280rpx;
    height: 280rpx;
    position: absolute;
    border-radius: 50%;
    background:rgb(252, 196, 196);
    clip: rect(0 280rpx 280rpx 140rpx);
    transform: rotate(0deg);
}
/* 圆环样式 */


/* 配对文字 */
.pair-text-panel{
    width: 560rpx;
    margin: 0 auto;
    font-size:40rpx;
    font-family:PingFangSC-Medium;
    font-weight: 600;
    color:rgba(51,51,51,1);
}
.pair-text-img{
    position: relative;
    display: inline-block;
    width: 0rpx;
    height: 0rpx;
    font-size: 0;
    margin-left: -10rpx;
}
.pair-text-panel .pair-text-img>image{
    position: absolute;
    top: -46rpx;
    left: -10rpx;
    width: 18rpx;
    height: 16rpx;
    max-width: none;
    max-height: none;
}
.pair-text-panel .pair-text-img:last-child>image{
    transform: rotate(180deg);
    top: auto;
    left: auto;
    right: -20rpx;
    bottom: -16rpx;
}
/* End配对文字 */

/* 指数 */
.heart{
    width: 100%;
    color: #E5E5E5;
}
.heart-panel{
    width: 646rpx;
    margin: 0 auto;
    display: flex;
    justify-content: center;
    align-items: center;
}
.heart-box{
    flex: 1;
    text-align: center;
}
.heart-box-panel{
    display: flex;
    justify-content: center;
    text-align: center;
    margin-top: 20rpx;
}
.heart-box>text{
    font-size:32rpx;
    font-family:PingFangSC-Regular;
    color:#666666;
    line-height:44rpx;
}
.heart-box-panel>view{
    width: auto;
    width: 48rpx;
    height: 40rpx;
    display: inline-block;
    color: transparent;
}
.heart-border{
    width: 4rpx;
    height: 40rpx;
    background: #E5E5E5;
}
/* End指数 */


/* 功能按钮组 */
.btn-group{
    position: fixed;
    width: 100%;
    height: 112rpx;
    bottom: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    border: 1rpx solid rgba(0,0,0,.1);
}
.ipx{
    padding-bottom: 34px;
}
.btn-home{
    width: 120rpx;
    height: 112rpx;
    border-bottom: 1rpx solid rgba(0,0,0,.1);
}
.btn-home image{
    display: block;
    width: 39rpx;
    height: 39rpx;
    margin: 20rpx auto 0 auto;
}
.btn-home text{
    display: block;
    text-align: center;
    font-size:24rpx;
    font-family:PingFangSC-Regular;
    color:rgba(102,102,102,1);
    line-height:34rpx;
}
.btn-pair{
    flex: 1;
    height: 112rpx;
    text-align: center;
    background:linear-gradient(-90deg,rgba(174,69,255,1),rgba(146,98,251,1));
}
.btn-pair button{
    font-size:36rpx;
    font-family:PingFangSC-Medium;
    color:rgba(255,255,255,1);
    line-height: 112rpx;
}
.btn-ipx{
    width: 100%;
    height: 34px;
    background: #fff;
    box-shadow:0px 0px .5px .5px rgba(229,229,229,1);
}